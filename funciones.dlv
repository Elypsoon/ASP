%Extra
%parejas(X,Y) :- +(X,Y, #maxint), #int(X), #int(Y), X<=Y.

%Extra 2.
%triple(X,Y,Z) :- *(X,X,X2), *(Y,Y,Y2), *(Z,Z,Z2), +(Y2,Z2,X2), #int(X), #int(Y), #int(Z).

